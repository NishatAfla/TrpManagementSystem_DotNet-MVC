
@{
    ViewBag.Title = "DeleteProgram";
}

<h2>DeleteProgram</h2>

    <p>Are you sure you want to delete this channel?</p>
    <p>
        <strong>@Model.ProgramName</strong>
    </p>
    <form method="post">
        <input type="hidden" name="Id" value="@Model.ProgramId" />
        <button type="submit" name="dcsn" value="Yes">Yes</button>
        <button type="submit" name="dcsn" value="No">No</button>
    </form>
        
